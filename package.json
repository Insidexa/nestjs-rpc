{
  "name": "@jashkasoft/nestjs-json-rpc",
  "version": "7.4.0",
  "description": "JSON RPC module for NestJS framework",
  "author": "jashka",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "package.json",
    "README.md",
    "dist/**/*",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@nestjs/common": "7.0.9",
    "@nestjs/core": "7.0.9",
    "@nestjs/platform-express": "7.0.9",
    "@nestjs/platform-fastify": "7.0.9",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.5",
    "@nestjs/testing": "7.0.9",
    "@types/express": "4.17.6",
    "@types/jest": "25.2.1",
    "@types/node": "12.12.37",
    "@types/supertest": "2.0.9",
    "jest": "25.5.3",
    "supertest": "4.0.2",
    "ts-jest": "25.4.0",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.2",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "test",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Insidexa/nestjs-rpc"
  },
  "bugs": {
    "email": "jashkasoft@gmail.com",
    "url": "https://github.com/Insidexa/nestjs-rpc/issues"
  }
}
